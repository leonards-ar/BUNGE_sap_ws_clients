<urn:ZtrRfcAbmOrdenCarga xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:urn="urn:sap-com:document:sap:soap:functions:mc-style">
         <Abm>${#abm#}</Abm>
         <ScabEntrega>
            <ZzordCarga>${#ordcarga#}</ZzordCarga>
            <Zzescenario>${#escenario#}</Zzescenario>
            <ZztipoTrans>${#tipotrans#}</ZztipoTrans>
            <ZzmercaPeli>${#mercapeli#}</ZzmercaPeli>
            <ZzfechaEjec>${#fechaejec#}</ZzfechaEjec>
            <Zzrecepcion>${#recepcion#}</Zzrecepcion>
            <Zzchofer>${#chofer#}</Zzchofer>
            <ZzchapaChasis>${#chapachasis#}</ZzchapaChasis>
            <ZzchapaAcoplado>${#chapaacoplado#}</ZzchapaAcoplado>
            <ZzestOrFeIn>${#estorfein#}</ZzestOrFeIn>
            <ZzestOrHsIn>${#estorhsin#}</ZzestOrHsIn>
            <ZzestOrFeFin>${#estorfefin#}</ZzestOrFeFin>
            <ZzestOrHsFin>${#estorhsfin#}</ZzestOrHsFin>
            <ZzestDeFeIn>${#estdefein#}</ZzestDeFeIn>
            <ZzestDeHsIn>${#estdehsin#}</ZzestDeHsIn>
            <ZzestDeFeFin>${#estdefefin#}</ZzestDeFeFin>
            <ZzestDeHsFin>${#estdehsfin#}</ZzestDeHsFin>
            <Motanul1>${#motanul1#}</Motanul1>
            <Motanul2>${#motanul2#}</Motanul2>
         </ScabEntrega>
         <TPosOrden>
	 ${loop(posorden)}
            <item>
               <Posnr>${#posnt#}</Posnr>
               <Matnr>${#matnt#}</Matnr>
               <Zzcantidad>${#cantidat#}</Zzcantidad>
               <Zzremito>${#remitt#}</Zzremito>
            </item>
	{end loop}
         </TPosOrden>
         <TPosent>
	 ${loop(posent)}
            <item>
               <Vbeln>${#vbeln#}</Vbeln>
               <Zznegocio>${#negocio#}</Zznegocio>
               <ZznegocioPos>${#negociopos#}</ZznegocioPos>
               <Matnr>${#matnr#}</Matnr>
               <Zzcantidad>${#cantidad#}</Zzcantidad>
               <Vrkme>${#vrkme#}</Vrkme>
               <Zzremito>${#remito#}</Zzremito>
               <Zzrepeso>${#repeso#}</Zzrepeso>
               <ZzzonaFleteO>${#zonafleteo#}</ZzzonaFleteO>
               <ZzpaisOrigen>${#paisorigen#}</ZzpaisOrigen>
               <ZzplantaO>${#plantao#}</ZzplantaO>
               <Zzorigen>${#origen#}</Zzorigen>
               <ZzsubOrigen>${#suborigen#}</ZzsubOrigen>
               <ZzpaisDestino>${#paisdestino#}</ZzpaisDestino>
               <ZzplantaD>${#plantad#}</ZzplantaD>
               <Zzdestino>${#destino#}</Zzdestino>
               <ZzsubDestino>${#subdestino#}</ZzsubDestino>
               <Matsap>${#matsap#}</Matsap>
               <Descmat>${#descmat#}</Descmat>
            </item>
	{end loop}
         </TPosent>
         <TTarifa>
            <item>
               <Posnr></Posnr>
               <Tarifa></Tarifa>
               <Moneda></Moneda>
            </item>
         </TTarifa>
         <TypeMod></TypeMod>
      </urn:ZtrRfcAbmOrdenCarga>
