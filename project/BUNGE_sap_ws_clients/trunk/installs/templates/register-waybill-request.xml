<way:registerWaybill  xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:way="http://waybillregistration.ws.industrysystem.com.ar/">
   <waybillEntity>
      <!--Optional:-->
      <addresseeDocument>${#addresseeDocument#}</addresseeDocument>
      <!--Optional:-->
      <commercialSenderDocument>${#commercialSenderDocument#}</commercialSenderDocument>
      <!--Optional:-->
      <commercialSenderName>${#commercialSenderName#}</commercialSenderName>
      <!--Optional:-->
      <destinationCommercialCode>${#destinationCommercialCode#}</destinationCommercialCode>
      <!--Optional:-->
      <destinationDocument>${#destinationDocument#}</destinationDocument>
      <!--Optional:-->
      <destinationLocation>${#destinationLocation#}</destinationLocation>
      <!--Optional:-->
      <grain>${#grain#}</grain>
      <!--Optional:-->
      <holderDocument>${#holderDocument#}</holderDocument>
      <!--Optional:-->
      <holderName>${#holderName#}</holderName>
      <!--Optional:-->
      <loadsDetail>
         <!--You have a CHOICE of the next 2 items at this level-->
         <truck>
            <!--Optional:-->
            <technology>${#truck_technology#}</technology>
            <!--Optional:-->
            <ctg>${#truck_ctg#}</ctg>
         </truck>
         <wagon>
            <!--Optional:-->
            <technology>${#wagon_technology#}</technology>
            <!--Optional:-->
            <wagonNumber>${#wagon_wagonNumber#}</wagonNumber>
            <!--Optional:-->
            <wagonsQty>${#wagon_wagonsQty#}</wagonsQty>
         </wagon>
      </loadsDetail>
      <!--Optional:-->
      <originLocation>${#originLocation#}</originLocation>
      <!--Optional:-->
      <waybillType>${#waybillType#}</waybillType>
      <!--Optional:-->
      <originEstablishment>${#originEstablishment#}</originEstablishment>
      <!--Optional:-->
      <waybillNumber>${#waybillNumber#}</waybillNumber>
   </waybillEntity>
</way:registerWaybill>