<urn:ZtrRfcAbmPedidoFlete xmlns:soapenv="http://schemas.xmlsoap.org/soap/envelope/" xmlns:urn="urn:sap-com:document:sap:soap:functions:mc-style">
         <Abm>${#abm#}</Abm>
         <ScabPedido>
            <Vbeln>${#vbeln#}</Vbeln>
            <Escenario>${#escenario#}</Escenario>
            <Cuit>${#cuit#}</Cuit>
            <Numneg>${#numneg#}</Numneg>
            <Usercom>${#usercom#}</Usercom>
            <Ryd>${#ryd#}</Ryd>
            <Pesorde>${#pesorde#}</Pesorde>
            <CuitTransp>${#cuittransp#}</CuitTransp>
		<Motanul1>${#motanul1#}</Motanul1>
            <Motanul2>${#motanul2#}</Motanul2>
         </ScabPedido>
         <TPosPedido>
	 ${loop(pospedidos)}
            <item>
               <Posnr>${#posnrcom#}</Posnr>
               <Tarifcom>${#tarifcom#}</Tarifcom>
               <Montarcom>${#montarcom#}</Montarcom>
               <FechaInicio>${#fechainicio#}</FechaInicio>
               <FechaFinal>${#fechafinal#}</FechaFinal>
               <Paisorig>${#paisorig#}</Paisorig>
               <Origen>${#origen#}</Origen>
               <SubOrigen>${#suborigen#}</SubOrigen>
               <Plantaorig>${#plantaorig#}</Plantaorig>
               <Paisdest>${#paisdest#}</Paisdest>
               <Destino>${#destino#}</Destino>
               <SubDestino>${#subdestino#}</SubDestino>
               <Plantadest>${#plantadest#}</Plantadest>
               <DifKm>${#difkm#}</DifKm>
               <Material>${#material#}</Material>
               <Cantidad>${#cantidad#}</Cantidad>
               <Unmed>${#unmed#}</Unmed>
               <Tiptransp>${#tiptransp#}</Tiptransp>
               <Mercpelig>${#mercpelig#}</Mercpelig>
               <Matsap>${#matsap#}</Matsap>
               <Descmat>${#descmat#}</Descmat>
            </item>
         {end loop}
         </TPosPedido>
         <TTarifa>
            <item>
               <Posnr>${#posnr#}</Posnr>
               <Tarifa>${#tarifa#}</Tarifa>
               <Moneda>${#moneda#}</Moneda>
            </item>
         </TTarifa>
      </urn:ZtrRfcAbmPedidoFlete>
